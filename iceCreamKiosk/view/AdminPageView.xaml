<UserControl x:Class="iceCreamKiosk.view.AdminPageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iceCreamKiosk.view"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="59*"/>
            <RowDefinition Height="91*"/>
        </Grid.RowDefinitions>
        <Button Command="{Binding BackCommand}" Content="Back" HorizontalAlignment="Left" Height="108" Margin="93,56,0,0" VerticalAlignment="Top" Width="291"/>
        <Button Command="{Binding NextCommand}" Content="Next" HorizontalAlignment="Left" Height="95" Margin="467,69,0,0" VerticalAlignment="Top" Width="251"/>
        <ContentControl Content="{Binding CurrentPage}" Margin="10,31.2,-10,9.6" Grid.Row="1">
            
        </ContentControl>
    </Grid>
</UserControl>
